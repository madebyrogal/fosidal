{% extends 'AdminBundle::base.html.twig' %}

{% block body %}
    <h1>Result</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ result.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ result.name }}</td>
            </tr>
            <tr>
                <th>Surname</th>
                <td>{{ result.surname }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ result.email }}</td>
            </tr>
            <tr>
                <th>Street</th>
                <td>{{ result.street }}</td>
            </tr>
            <tr>
                <th>Housenr</th>
                <td>{{ result.houseNr }}</td>
            </tr>
            <tr>
                <th>Flatnr</th>
                <td>{{ result.flatNr }}</td>
            </tr>
            <tr>
                <th>Postcode</th>
                <td>{{ result.postCode }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ result.city }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ result.phone }}</td>
            </tr>
            <tr>
                <th>Agree1</th>
                <td>{% if result.agree1 %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Agree2</th>
                <td>{% if result.agree2 %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Points</th>
                <td>{{ result.points }}</td>
            </tr>
            <tr>
                <th>Content</th>
                <td>{{ result.content }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{% if result.created %}{{ result.created|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Award1</th>
                <td>{% if result.award1 %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Award2</th>
                <td>{% if result.award2 %}Yes{% else %}No{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_result_index') }}">Back to the list</a>
        </li>
    </ul>
{% endblock %}
